#include<stdio.h>
int main(){
	int a,b;
	scanf("%d",&a);
	printf("%o\n",a);
	printf("%x\n",a);
	b=a%5;
	printf("%d\n",b);
	printf("\n");
	return 0;
}
#include<stdio.h>
int main(){
	float a,b;
	scanf("%f%f",&a,&b);
	printf("%.2f\n",a+b);
	printf("%.2f\n",a-b);
	printf("%.2f\n",a*b);
	printf("%.2f\n",a/b);
	return 0;
}
#include<stdio.h>
#define pi 3.14159
int main(){
	double r,h;
	scanf("%lf%lf",&r,&h);
	printf("%lf\n",4*pi*r*r);
	printf("%lf\n",(4*pi*r*r*r)/3);

	return 0;
}
#include<stdio.h>
int main(){
	char a;
	scanf("%c",&a);
	if(97<a&&a<122)
	{
	
	printf("%c\n",a-1);
	printf("%c\n",a);
	printf("%c\n",a+1);
}
	else
	printf("输入错误");
	return 0;
}
#include<stdio.h>
#include<math.h> 
int main(){
	int a,b,c;
	scanf("%d%d%d",&a,&b,&c);
	int p,s,r,wai;
	p=(a+b+c)/2;
	s=sqrt(p*(p-a)*(p-b)*(p-c));
	r=2*s/(a+b+c);
	wai=(a*b*c)/(4*s);
	printf("%d\n%d",r,wai);
	return 0;
}
#include<stdio.h>
int main(){
	int ab,bc,ac,biao,a,b,c;
	scanf("%d%d%d",&ab,&bc,&ac);
	biao=ab*bc*ac;
	a=biao/(bc*bc);
	b=biao/(ac*ac);
	c=biao/(ab*ab);
	printf("%d\n",4*(a+b+c));
	return 0;
}
#include<stdio.h>
int main(){
	double hua,she;
	printf("输入华氏温度：\n");
	scanf("%lf",&hua);
	she=(5*(hua-32))/9; 
	printf("%.2f\n",she);
	return 0;
}
#include<stdio.h>
int huan(int x,int y)
{
	int z;
	z=x;
	x=y;
	y=z;
	printf("%d\n%d",x,y);
}
int main(){
	int a,b;
	scanf("%d%d",&a,&b);
	huan(a,b);
	return 0;
}
#include<stdio.h>
int main(){
	int a,b,c;
	a=40000/3600;
	b=(40000%3600)/60;
	c=((40000%3600)%60);
	printf("%d小时%d分钟%d秒\n",a,b,c);
	return 0;
}
#include<stdio.h>
int main(){
	int h,f,a,b;
	scanf("%d\n%d",&h,&f);
	b=(f-2*h)/2;
	a=(4*h-f)/2;
	printf("鸡:%d兔:%d\n",a,b);
	return 0;
}
#include<stdio.h>
int main(){
	float a,b,c;
	scanf("%f%f%f",&a,&b,&c);
    	if(a+b>c&&a+c>b&&b+c>b)
	{
	printf("能"); }
	else printf("不能\n");
	return 0;
}
#include<stdio.h>
#include<math.h> 
int main(){
	float x,y;
	scanf("%f",&x);
	if(x>0&&x<1)
	y=x; 
	else if (x>=3&&x<=4)
	y=x+2;
	else{
	y=sin(x*x);}
	printf("%f\n",y);
	return 0;
}
#include<stdio.h>
int main(){
	char a;
	scanf("%c",&a);
		if((97<=a&&a<=116)||(65<=a&&a<=84))
	{
		printf("%c\n",a+6);
}
	else if(117<=a&&a<=122)
	printf("%c\n",96+(a-116));
	else if(85<=a&&a<=90)
	printf("%c\n",64+(a-84));
	else
	printf("输入错误");
	printf("\n");
	return 0;
}
